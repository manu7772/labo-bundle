{
    "name": "aequation/labo",
    "type": "symfony-bundle",
    "description": "This bundle is my standard starter for all my Symfony projects.",
    "keywords": [ "symfony" ],
    "homepage": "https://www.aequation.fr",
    "license": "MIT",
    "authors": [
        {
            "name": "Emmanuel Dujardin / Aequation",
            "homepage": "https://github.com/manu7772"
        }
    ],
    "autoload": {
        "psr-4": {
            "Aequation\\LaboBundle\\": "src/"
        }
    },
    "require": {
        "php": ">=8.2",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "doctrine/doctrine-bundle": "^2.11",
        "doctrine/doctrine-migrations-bundle": "^3.3",
        "doctrine/orm": "^3.2",
        "easycorp/easyadmin-bundle": "*",
        "knplabs/knp-paginator-bundle": "^6.3",
        "liip/imagine-bundle": "*",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpstan/phpdoc-parser": "^1.25",
        "symfony/asset": "7.1.*",
        "symfony/asset-mapper": "7.1.3",
        "symfony/console": "7.1.*",
        "symfony/doctrine-messenger": "7.1.*",
        "symfony/dotenv": "7.1.*",
        "symfony/expression-language": "7.1.*",
        "symfony/filesystem": "7.1.*",
        "symfony/flex": "^2",
        "symfony/form": "7.1.*",
        "symfony/framework-bundle": "7.1.*",
        "symfony/http-client": "7.1.*",
        "symfony/intl": "7.1.*",
        "symfony/mailer": "7.1.*",
        "symfony/mime": "7.1.*",
        "symfony/monolog-bundle": "^3.0",
        "symfony/notifier": "7.1.*",
        "symfony/process": "7.1.*",
        "symfony/property-access": "7.1.*",
        "symfony/property-info": "7.1.*",
        "symfony/rate-limiter": "7.1.*",
        "symfony/runtime": "7.1.*",
        "symfony/security-bundle": "7.1.*",
        "symfony/serializer": "7.1.*",
        "symfony/stimulus-bundle": "^2.13",
        "symfony/string": "7.1.*",
        "symfony/translation": "7.1.*",
        "symfony/twig-bundle": "7.1.*",
        "symfony/ux-live-component": "^2.14",
        "symfony/ux-toggle-password": "^2.14",
        "symfony/ux-turbo": "^2.13",
        "symfony/ux-twig-component": "^2.14",
        "symfony/validator": "7.1.*",
        "symfony/web-link": "7.1.*",
        "symfony/yaml": "7.1.*",
        "symfonycasts/dynamic-forms": "^0.1.1",
        "symfonycasts/tailwind-bundle": "^0.6.0",
        "symfonycasts/verify-email-bundle": "^1.16",
        "twig/cache-extra": "^3.8",
        "twig/extra-bundle": "^2.12|^3.0",
        "twig/intl-extra": "^3.8",
        "twig/twig": "^2.12|^3.0",
        "vich/uploader-bundle": "^2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "symfony/browser-kit": "7.1.*",
        "symfony/css-selector": "7.1.*",
        "symfony/debug-bundle": "7.1.*",
        "symfony/maker-bundle": "^1.0",
        "symfony/phpunit-bridge": "^7.0",
        "symfony/stopwatch": "7.1.*",
        "symfony/web-profiler-bundle": "7.1.*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd",
            "tailwind:build": "symfony-cmd",
            "asset-map:compile": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "minimum-stability": "dev"
}
